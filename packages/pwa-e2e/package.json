{
  "name": "pwa-e2e",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo don't forget about local build: 'npm run build' && testcafe chrome ./lib/tests/*.spec.js --reporter html --screenshots-on-fails --speed 0.1",
    "test:debug": "npm test testcafe -- --inspect-brk --dev",
    "test:ci": "mkdir screenshots && npm run build && testcafe 'chrome:headless' ./lib/tests/*.spec.js --reporter html --screenshots-on-fails",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc",
    "build:js": "./node_modules/.bin/babel src --out-dir lib --extensions \".ts\" --source-maps inline",
    "build:js:watch": "./node_modules/.bin/babel src --out-dir lib --watch --extensions \".ts\" --source-maps inline",
    "lint": "./node_modules/.bin/tslint",
    "testcafe": "node node_modules/testcafe/bin/testcafe.js",
    "docs": "node ./node_modules/typedoc/bin/typedoc --options ./typedoc.js ./src"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "7.1.2",
    "@babel/core": "7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "7.1.0",
    "@babel/preset-typescript": "7.1.0",
    "@types/chrome": "0.0.75",
    "@types/dotenv": "^4.0.3",
    "@types/faker": "^4.1.4",
    "@types/node": "^10.12.2",
    "babel-plugin-module-resolver": "^3.1.1",
    "dotenv": "^6.1.0",
    "faker": "^4.1.0",
    "reflect-metadata": "^0.1.12",
    "testcafe": "^0.23.0",
    "testcafe-react-selectors": "^3.0.0",
    "testcafe-reporter-html": "^1.3.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typedoc": "^0.13.0",
    "typescript": "^3.1.6"
  },
  "dependencies": {}
}
